{
    "name": "manymailerplus",
    "version": "1.5.0",
    "description": "ExpressionEngine Extension for mass transactional where emails are consumed from csv.",
    "main": "javascript/compose.js",
    "scripts": {
        "build": "npx node_modules/webpack --mode development -d --progress --colors --watch",
        "start": "webpack-dev-server --mode development --open",
        "dev": "webpack --mode development -d --progress --colors --watch --module-bind js=babel-loader",
        "prod": "npx node_modules/webpack --mode production -d --progress --colors --watch  --module-bind js=babel-loader",
        "test": "mocha --opts=./spec/mocha.opts ./spec/**/*.spec.js",
        "nyc-test": "nyc --reporter=html --reporter=text mocha spec/*.js",
        "coverage": "nyc report --reporter=text-lcov | coveralls",
        "wdio": "wdio wdio.conf.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/StolenThunda/manymailerplus.git"
    },
    "keywords": [
        "ExpressionEngine"
    ],
    "author": "Antonio Moses",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/StolenThunda/manymailerplus/issues"
    },
    "homepage": "https://github.com/StolenThunda/manymailerplus#readme",
    "devDependencies": {
        "@rpii/wdio-html-reporter": "^0.8.0",
