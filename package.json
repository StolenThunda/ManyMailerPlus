        "@wdio/browserstack-service": "^5.12.5",
        "@wdio/cli": "^5.13.1",
        "@wdio/concise-reporter": "^5.12.1",
        "@wdio/devtools-service": "^5.13.0",
        "@wdio/dot-reporter": "^5.12.1",
