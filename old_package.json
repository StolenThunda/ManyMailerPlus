{
    "name": "manymailerplus",
    "version": "1.5.0",
    "description": "ExpressionEngine Extension for mass transactional where emails are consumed from csv.",
    "main": "javascript/compose.js",
    "scripts": {
        "start": "webpack-dev-server --mode development --open",
        "dev": "npx node_modules/webpack --mode development -d --progress --colors --watch --module-bind js=babel-loader",
        "prod": "npx node_modules/webpack --mode production -d --progress --colors --watch  --module-bind js=babel-loader",
        "test": "mocha --recursive --reporter list tests/**/*.js",
        "nyc-test": "nyc --reporter=html --reporter=text mocha tests/*.js",
        "coverage": "nyc report --reporter=text-lcov | coveralls"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/StolenThunda/manymailerplus.git"
    },
    "keywords": [
        "ExpressionEngine"
    ],
    "author": "Antonio Moses",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/StolenThunda/manymailerplus/issues"
    },
    "homepage": "https://github.com/StolenThunda/manymailerplus#readme",
    "devDependencies": {
        "@babel/core": "^7.6.0",
        "@babel/preset-env": "^7.6.0",
        "babel-loader": "^8.0.6",
        "chai": "^4.2.0",
        "coveralls": "^3.0.6",
        "css-loader": "^3.2.0",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "jquery": "^3.4.1",
        "mini-css-extract-plugin": "^0.8.0",
        "mocha": "^6.2.0",
        "nyc": "^14.1.1",
        "ts-loader": "^6.0.0",
        "webpack": "^4.31.0",
        "webpack-cli": "^3.3.2"
    }
}